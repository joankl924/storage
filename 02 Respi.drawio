        <mxCell id="2zh8jBSJfKitzomMzvb2-11" value="Sarcoidosis" style="rounded=1;whiteSpace=wrap;html=1;fillColor=#e1d5e7;strokeColor=#9673A6;align=left;spacingLeft=2;fontStyle=1;fontSize=11;" vertex="1" parent="1">
