        <mxCell id="hQn2WTLAaTq2nDV0td08-1" value="SCLC" style="rounded=1;whiteSpace=wrap;html=1;spacingTop=0;fontStyle=1;align=left;spacing=5;fillColor=#ffd23f;fontColor=#000000;strokeColor=#ffd23f;" vertex="1" parent="1">
