        <mxCell id="fCBD8a2lisjKOdRzJ-kr-1" value="Breast cancer" style="rounded=1;whiteSpace=wrap;html=1;spacingTop=0;fontStyle=1;align=left;spacing=5;fillColor=#DDC3ED;strokeColor=#675A6E;" parent="1" vertex="1">
